{
  "name": "youzi-bot",
  "version": "0.0.49",
  "description": "一個實用、有趣的機器人",
  "main": "bot.js",
  "engines": {
    "node": "^16.x"
  },
  "scripts": {
    "start": "echo \">>>>>> 啟動分片！<<<<<<\" && node index.js",
    "exit": "echo \"Error: 沒作用\" && exit 0",
    "test": "CI=true node --trace-deprecation index.js",
    "codespaces": "token=$TOKEN clientID=$CLIENTID clientSECRET=$CLIENTSECRET node index.js",
    "webpack": "webpack -d -w",
    "updates": "npm i && echo \"開始更新\"",
    "lint": "eslint .",
    "dev": "npm i -g npm-check-updates eslint",
    "check": "npm run update && npm run lint",
    "reset": "echo \"刪除所有命令\" && node reset.js",
    "update_patch": "npm version patch",
    "update_minor": "npm version minor",
    "update_major": "npm version major",
    "beta_run": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Youzi9601/YZBot.git"
  },
  "keywords": [
    "Yes"
  ],
  "author": "柚子Youzi",
  "license": "GNU GPL",
  "bugs": {
    "url": "https://github.com/Youzi9601/YZBot/issues"
  },
  "homepage": "https://github.com/Youzi9601/YZBot#readme",
  "dependencies": {
    "axios": "^1.3.4",
    "better-sqlite3": "^8.2.0",
    "chalk": "^4.0.0",
    "child_process": "^1.0.2",
    "cli-box": "^6.0.10",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "discord-giveaways": "^6.0.1",
    "discord-html-transcripts": "^3.1.3",
    "discord-player": "^6.0.0",
    "discord-together": "^1.3.31",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.0",
    "glob": "^9.2.1",
    "html-entities": "^2.3.3",
    "humanize-duration": "^3.28.0",
    "mathjs": "^11.6.0",
    "moment": "^2.29.4",
    "mongoose": "^7.0.1",
    "ms": "^2.1.3",
    "node-fetch": "^3.3.0",
    "node-gyp": "^9.3.1",
    "openai": "^3.2.1",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "quick.db": "^9.0.8",
    "string-progressbar": "^1.0.4"
  },
  "auto-updater": {
    "repo": "/Youzi9601/YZBot",
    "branch": "master"
  },
  "devDependencies": {
    "eslint": "^8.35.0"
  }
